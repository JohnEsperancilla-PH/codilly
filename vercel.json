{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/public/index.php"
      }
    ],
    "builds": [
      {
        "src": "api/**/*.php",
        "use": "@vercel/php"
      },
      {
        "src": "public/index.php",
        "use": "@vercel/php"
      }
    ]
}
